    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "text-encoding": "^0.7.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
